{
  "extends": "@istanbuljs/nyc-config-typescript",
  "include": ["src"],
  "check-coverage": true,
  "per-file": true,
  "produce-source-map": true,
  "statements": 0,
  "functions": 0,
  "branches": 0,
  "lines": 0,
  "reporter": ["lcov", "text-summary", "html"],
  "all": true,
  "cache": true,
  "report-dir": "./coverage",
  "tmp-directory": ".nyc_output"
}
