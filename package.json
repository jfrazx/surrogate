    "nyc": "nyc",
    "precommit": "npm run coverage:test && npm run coverage:check",
    "prepush": "npm run coverage:test",
    "publish": "npx semantic-release",
    "semantic-release": "semantic-release",
    "semantic:cli": "semantic-release-cli"
  },
  "author": "jfrazx",
  "license": "MIT",
  "dependencies": {
    "@jfrazx/asarray": "^1.0.0",
    "@status/defaults": "^1.2.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/sinon": "^7.5.0",
    "chai": "^4.1.2",
    "codecov": "^3.6.1",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^3.0.9",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "semantic-release": "^15.13.27",
    "semantic-release-cli": "^5.2.1",
    "sinon": "^7.5.0",
    "sloc": "^0.2.0",
    "source-map-support": "^0.5.13",
    "travis-deploy-once": "^5.0.11",
    "ts-node": "^8.4.1",
    "tslint": "^5.11.0",
    "typescript": "^3.6.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
