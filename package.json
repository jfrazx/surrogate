{
  "name": "surrogate",
  "version": "1.0.0",
  "description": "Method hooking using ES2015 Proxy",
  "main": "dist/index.js",
  "scripts": {
    "sloc": "sloc ./src ",
    "commit": "git-cz",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "tsc": "tsc",
    "prebuild": "rimraf build",
    "build": "tsc",
    "prebuild:examples": "rimraf build-examples && npm run build",
    "build:examples": "tsc -p ./examples/tsconfig.json",
    "coverage:check": "nyc check-coverage",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov",
    "coverage:post": "codecov",
    "coverage:test": "nyc mocha",
    "nyc": "nyc",
    "precommit": "npm run coverage:test && npm run coverage:check",
    "prepush": "npm run coverage:test",
    "publish": "npx semantic-release",
    "postinstall": "npm run build",
    "semantic-release": "semantic-release",
    "semantic:cli": "semantic-release-cli"
  },
  "author": "jfrazx",
  "license": "MIT",
  "dependencies": {
    "@jfrazx/asarray": "^1.0.0",
    "@status/defaults": "^1.2.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^8.0.1",
    "@types/sinon": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "chai": "^4.1.2",
    "codecov": "^3.6.1",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "mocha": "^8.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.2.3",
    "semantic-release-cli": "^5.2.1",
    "sinon": "^9.0.2",
    "sloc": "^0.2.0",
    "source-map-support": "^0.5.13",
    "travis-deploy-once": "^5.0.11",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "mocha": {
    "recursive": true,
    "spec": "test/*.spec.ts",
    "reporter": "spec",
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ]
  }
}
