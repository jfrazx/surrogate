{
  "name": "surrogate",
  "version": "1.5.0",
  "description": "Object method hooks made easy",
  "main": "build/index.js",
  "keywords": [
    "hooks",
    "hook",
    "proxy",
    "hooking",
    "pre",
    "post",
    "flow",
    "control",
    "async",
    "sync",
    "api"
  ],
  "scripts": {
    "sloc": "sloc ./src",
    "commit": "git-cz",
    "test": "cross-env TS_NODE_PROJECT='./test/tsconfig.json' mocha",
    "test:watch": "mocha --watch",
    "tsc": "tsc",
    "predocs": "rimraf docs",
    "docs": "typedoc src/index.ts",
    "del:merged": "node -r ts-node/register/transpile-only scripts/merged",
    "leak:test": "node -r ts-node/register/transpile-only scripts/leak",
    "prebuild": "rimraf build",
    "build": "tsc",
    "prebuild:examples": "rimraf build-examples && npm run build",
    "build:examples": "tsc -p ./examples/tsconfig.json",
    "coverage:check": "nyc check-coverage",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov",
    "coverage:post": "codecov",
    "precoverage:test": "rimraf coverage",
    "coverage:test": "cross-env TS_NODE_PROJECT='./test/tsconfig.json' nyc mocha",
    "nyc": "nyc",
    "precommit": "npm run coverage:test && npm run coverage:check",
    "prepush": "npm run coverage:test",
    "prepublish": "npm run build",
    "semantic-release": "semantic-release",
    "semantic:cli": "semantic-release-cli",
    "mocha": "mocha"
  },
  "author": "jfrazx",
  "license": "MIT",
  "dependencies": {
    "@jfrazx/asarray": "^1.0.1",
    "@status/defaults": "^1.3.3",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.0",
    "@types/fast-redact": "^3.0.1",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.4",
    "@types/reflect-metadata": "^0.1.0",
    "@types/sinon": "^10.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "applicationinsights": "^2.2.1",
    "chai": "^4.3.6",
    "codecov": "^3.8.3",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fast-redact": "^3.1.0",
    "husky": "^7.0.4",
    "mocha": "^9.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "semantic-release-cli": "^5.4.4",
    "sinon": "^13.0.1",
    "sloc": "^0.2.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.5.0",
    "typedoc": "^0.22.12",
    "typescript": "^4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "mocha": {
    "recursive": true,
    "spec": "test/*.spec.ts",
    "reporter": "spec",
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jfrazx/surrogate.git"
  }
}
