{
  "name": "surrogate",
  "version": "1.3.2",
  "description": "Object method hooks made easy",
  "main": "build/index.js",
  "keywords": [
    "hooks",
    "hook",
    "proxy",
    "hooking",
    "pre",
    "post",
    "flow",
    "control",
    "async",
    "sync"
  ],
  "scripts": {
    "sloc": "sloc ./src ",
    "commit": "git-cz",
    "test": "cross-env TS_NODE_PROJECT='./test/tsconfig.json' mocha",
    "test:watch": "mocha --watch",
    "tsc": "tsc",
    "predocs": "rimraf docs",
    "docs": "typedoc src/index.ts",
    "prebuild": "rimraf build",
    "build": "tsc",
    "prebuild:examples": "rimraf build-examples && npm run build",
    "build:examples": "tsc -p ./examples/tsconfig.json",
    "coverage:check": "nyc check-coverage",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov",
    "coverage:post": "codecov",
    "precoverage:test": "rimraf coverage",
    "coverage:test": "cross-env TS_NODE_PROJECT='./test/tsconfig.json' nyc mocha",
    "nyc": "nyc",
    "precommit": "npm run coverage:test && npm run coverage:check",
    "prepush": "npm run coverage:test",
    "prepublish": "npm run build",
    "publish": "npx semantic-release",
    "semantic-release": "semantic-release",
    "semantic:cli": "semantic-release-cli"
  },
  "author": "jfrazx",
  "license": "MIT",
  "dependencies": {
    "@jfrazx/asarray": "^1.0.1",
    "@status/defaults": "^1.2.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.1",
    "@types/sinon": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "applicationinsights": "^2.0.0",
    "chai": "^4.3.4",
    "codecov": "^3.8.2",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.3",
    "semantic-release-cli": "^5.4.3",
    "sinon": "^11.1.1",
    "sloc": "^0.2.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.0.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.3.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "mocha": {
    "recursive": true,
    "spec": "test/*.spec.ts",
    "reporter": "spec",
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jfrazx/surrogate.git"
  }
}
