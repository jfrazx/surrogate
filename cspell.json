{
  "version": "0.2",
  "ignorePaths": [
    "node_modules",
    ".git",
    ".vscode",
    "package-lock.json",
    "package.json",
    "tsconfig.json",
    "serverless.yml",
    "config",
    "*.js",
    "dist",
    "*.csv",
    "*.log",
    "Makefile",
    ".webpack",
    "txt",
    "tmp"
  ],
  "words": [
    "codecov",
    "deregisters",
    "esnext",
    "klass",
    "lcov",
    "prettierrc",
    "proxied",
    "redactable",
    "sloc"
  ],
  "ignoreRegExpList": [
    "/\"(pre|post)[a-zA-Z]+\":/",
    "from '(@|\\./)?[/a-zA-Z0-9]+'",
    "/\"name\": \"[a-zA-Z]+\"/",
    "/\"@?[a-zA-Z0-9]+(/[a-zA-Z0-9]+)?\": \"\\^?[\\d+\\.]+\"/",
    "/YY[YMD hms]+/"
  ]
}
